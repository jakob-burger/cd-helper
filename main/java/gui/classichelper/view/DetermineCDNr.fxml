<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="669.0" prefWidth="569.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.gui.classichelper.controller.DetermineCDNrController">

    <children>
        <Label layoutX="144.0" layoutY="27.0" prefHeight="64.0" prefWidth="280.0" text="Determine CD Nr" textAlignment="CENTER">
            <font>
                <Font size="35.0" />
            </font>
        </Label>
        <TableView fx:id="albumBefore" layoutX="21.0" layoutY="105.0" prefHeight="500.0" prefWidth="525.0">
            <columns>
                <TableColumn fx:id="albumColumn" prefWidth="160.0" text="Album">
                </TableColumn>
                <TableColumn fx:id="titleColumn" prefWidth="140.0" text="Title">
                </TableColumn>
                <TableColumn fx:id="artistColumn" prefWidth="160.0" text="Artist">
                </TableColumn>
                <TableColumn fx:id="discNrColumn" prefWidth="50.0" text="Disc Nr">
                </TableColumn>
            </columns>
        </TableView>
        <Label layoutX="345.0" layoutY="626.0" prefHeight="17.0" prefWidth="50.0" text="CD Nr" />
        <TextField fx:id="CDNrTextField" layoutX="390.0" layoutY="622.0" prefHeight="25.0" prefWidth="35.0" />
        <ImageView fx:id="doubleTickImageView" fitHeight="48.0" fitWidth="48.0" layoutX="445.0" layoutY="611.0" onMouseClicked="#handleDoubleTickButton" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@/main/resources/img/icons8-double-tick-96.png" />
            </image>
        </ImageView>
        <ImageView fx:id="crossImageView" fitHeight="48.0" fitWidth="48.0" layoutX="507.0" layoutY="611.0" onMouseClicked="#handleCrossButton" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@/main/resources/img/icons8-close-window-96.png" />
            </image>
        </ImageView>
    </children>

</AnchorPane>
