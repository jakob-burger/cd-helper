<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="450.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.gui.controller.DeleteNonAudiofilesController">
    <children>
        <Label layoutX="20.0" layoutY="14.0" prefHeight="64.0" prefWidth="260.0" text="Delete Non Audio Files" textAlignment="CENTER">
            <font>
                <Font size="25.0" />
            </font>
        </Label>
        <TableView fx:id="files" layoutX="15.0" layoutY="78.0" prefHeight="300.0" prefWidth="275.0">
            <columns>
                <TableColumn fx:id="filename" prefWidth="260.0" text="Filename">
                </TableColumn>
            </columns>
        </TableView>
        <ImageView fx:id="singleTickImageView" fitHeight="48.0" fitWidth="48.0" layoutX="20.0" layoutY="388.0" onMouseClicked="#handleSingleTickButton"  pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@/main/resources/img/icons8-checkmark-96.png" />
            </image>
        </ImageView>
        <ImageView fx:id="doubleTickImageView" fitHeight="48.0" fitWidth="48.0" layoutX="125.0" layoutY="388.0" onMouseClicked="#handleDoubleTickButton"  pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@/main/resources/img/icons8-double-tick-96.png" />
            </image>
        </ImageView>
        <ImageView fx:id="crossImageView" fitHeight="48.0" fitWidth="48.0" layoutX="230.0" layoutY="388.0" onMouseClicked="#handleCrossButton" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@/main/resources/img/icons8-close-window-96.png" />
            </image>
        </ImageView>
    </children>
</AnchorPane>
